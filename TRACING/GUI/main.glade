<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.24"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="Main">
    <property name="width_request">450</property>
    <property name="height_request">240</property>
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="title" translatable="yes">Tracing 0.1</property>
    <property name="resizable">False</property>
    <property name="window_position">center</property>
    <property name="default_width">450</property>
    <property name="default_height">210</property>
    <property name="icon">T.ico</property>
    <signal name="delete-event" handler="gtk_main_quit" swapped="no"/>
    <child>
      <object class="GtkLayout" id="layout1">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <child>
          <object class="GtkLabel" id="label1">
            <property name="width_request">402</property>
            <property name="height_request">20</property>
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="label" translatable="yes">Seguimiento de Personas en Situación de Dependencia</property>
            <attributes>
              <attribute name="weight" value="bold"/>
              <attribute name="underline" value="True"/>
            </attributes>
          </object>
          <packing>
            <property name="x">15</property>
            <property name="y">11</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btAnadir">
            <property name="label" translatable="yes">Añadir Seguimiento</property>
            <property name="width_request">200</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="use_action_appearance">False</property>
            <signal name="clicked" handler="on_btAnadir_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="x">200</property>
            <property name="y">61</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btBuscar">
            <property name="label" translatable="yes">Buscar Seguimiento</property>
            <property name="width_request">200</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="use_action_appearance">False</property>
            <signal name="clicked" handler="on_btBuscar_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="x">200</property>
            <property name="y">100</property>
          </packing>
        </child>
        <child>
          <object class="GtkImage" id="image1">
            <property name="width_request">173</property>
            <property name="height_request">168</property>
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="pixbuf">logo.png</property>
          </object>
          <packing>
            <property name="x">10</property>
            <property name="y">45</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btConsultar">
            <property name="label" translatable="yes">Consultar registro</property>
            <property name="width_request">200</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="use_action_appearance">False</property>
            <signal name="clicked" handler="on_btConsultar_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="x">200</property>
            <property name="y">139</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btEstado">
            <property name="label" translatable="yes">Altas/Bajas Residentes</property>
            <property name="width_request">200</property>
            <property name="height_request">30</property>
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="use_action_appearance">False</property>
            <signal name="clicked" handler="on_btEstado_clicked" swapped="no"/>
          </object>
          <packing>
            <property name="x">200</property>
            <property name="y">178</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
